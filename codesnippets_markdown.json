{
    "Todo Shortcode": {
        "prefix": ["todo", "shortcode"],
        "body": ["{{<todo>}}\n{{</todo>}}"],
        "description": "For my Hugo todo Shortcode"
    },
    "Info Shortcode": {
        "prefix": ["info", "shortcode"],
        "body": ["{{<info>}}\n{{</info>}}"],
        "description": "For my Hugo Info Shortcode"
    },
    "Two Columns Shortcode": {
        "prefix": ["column", "two column", "layout", "shortcode"],
        "body": ["{{<twoculumn>}}\n{{<left 50>}}\nCONTENT LEFT\n{{</left>}}\n{{<right>}}\nCONTENT RIGHT\n{{</right 50>}}\n{{</twoculumn>}}"],
        "description": "For my Hugo two-column-layout Shortcode"
    },
    "console": {
        "prefix": ["console"],
        "body": ["{{<console 700px>}}\nCONTENT. ESCAPE < WITH &lt; AND > WITH &gt;\n{{</console>}}"],
        "description": "For my Hugo console Shortcode"
    },
    "table title only": {
        "prefix": ["table title only"],
        "body": ["{{<console 700px>}}\nCONTENT. ESCAPE < WITH &lt; AND > WITH &gt;\n{{</console>}}"],
        "description": "For tables with a title only"
    },
    "doclink": {
        "prefix": ["doclink"],
        "body": ["{{<doclink \"url\" >}}"],
        "description": "For my Hugo doclink Shortcode"
    },
}